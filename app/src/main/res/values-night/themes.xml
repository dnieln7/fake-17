<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.Fake17" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/dark21</item>
        <item name="colorPrimaryVariant">@color/dark12</item>
        <item name="colorOnPrimary">@color/white</item>

        <item name="colorSecondary">@color/secondaryLight</item>
        <item name="colorSecondaryVariant">@color/secondaryLight</item>
        <item name="colorOnSecondary">@color/dark32</item>

        <item name="background">@color/dark21</item>
        <item name="android:textColor">@color/textOnDarkPrimary</item>

        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.Fake17.Splash">
        <item name="android:windowBackground">@drawable/splash_screen</item>
    </style>

    <style name="Theme.Fake17.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>